.profile {
  width: 100%;
  flex-grow: 1;
  padding-top: 104px;

  &__sub-button {
    color: var(--blue-600);
    text-decoration: none;
  }

  &__settings {
    min-width: 200px;
  }

  &__logo {
    max-height: 100%;
  }

  &__logo-container {
    width: 100px;
    height: 100px;
    overflow: hidden;
    border-radius: 50%;
  }
}
