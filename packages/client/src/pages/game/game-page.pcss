.game-page {
  width: 100%;
  padding-top: 96px;
  flex-grow: 1;

  &__container {
    width: fit-content;
    height: fit-content;
    position: relative;
  }

  &__overlay {
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: var(--gray-900);
    border-radius: 16px;
  }
}
