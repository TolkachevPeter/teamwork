	"scripts": {
		"dev": "cross-env EXTERNAL_SERVER_URL='http://localhost:3001' INTERNAL_SERVER_URL='http://localhost:3001' NODE_ENV=development TS_NODE_PROJECT=tsconfig.server.json nodemon server/index.ts",
		"build": "tsc --project tsconfig.server.json && vite build && vite build --ssr src/entry-server.tsx --outDir dist/server",
		"preview": "node server/index.js",
		"lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --cache --cache-location ./node_modules/.cache/eslint",
		"lint:stylelint": "stylelint \"src/**/*.{css,pcss}\" --cache --cache-location ./node_modules/.cache/stylelint",
		"format": " yarn format:prettier && yarn format:eslint && yarn format:stylelint",
		"format:prettier": "prettier --write .",
		"format:eslint": "eslint --fix --ext .ts,.tsx",
		"format:stylelint": "stylelint --fix \"src/**/*.{css,pcss}\"",
		"test": "jest ./",
		"start:doc": "mdx-deck presentation.mdx"
	},
